{
  "allowext": [
      "png",
      "jpg",
      "jpeg",
      "gif",
      "css",
      "js",
      "swf",
      "ogv",
      "ogg",
      "mp4",
      "webm",
      "html",
      "ico",
      "svg"
  ],
  "rules": {
      "cdn": {
          "source": "./dist/cdn/",
          "target": "./cdn/",
          "recursive": 1,
          "compress": 0
      },
      "webserver": {
          "source": "./dist/webserver/",
          "target": "./webserver/",
          "recursive": 1,
          "compress": 0
      },
      "offline": {
          "source": "./dist/offline/offline.zip",
          "target": "./offline/offline.zip",
          "recursive": 1,
          "compress": 0
      },
      "nodejs": {
          "source": "./dist/node/",
          "target": "./node/",
          "recursive": 1,
          "compress": 0
      }
  }
}
